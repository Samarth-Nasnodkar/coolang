object Pet {
  field nick

  constructor(_nick) {
    nick = _nick
  }
}

object Person {
  field name
  field pet

  constructor(_name, _pet) {
    name = _name
    pet = _pet
  }

  fn to_string() {
    return "Hi! my pet is " + pet.nick
  }
}

let _per = Person("Maya", Pet("Max"))
println(_per.to_string())