object person {
  field name
  field age

  constructor(_name, _age) {
    name = _name
    age = _age
  }

  fn printData() {
    print("Name =", name, ", Age = ", age)
  }

  fn updateName(_name) {
    name = _name
  }

}

fn makeNew(_p) {
  return _p
}

let p = person("Samarth", 19)
p.printData()
let k = makeNew(p)
k.printData()
p.updateName("Raj")
p.printData()
k.printData()